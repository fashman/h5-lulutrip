{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8", "iOS >= 6", "Android > 4.1", "Firefox > 20"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx", 
    "transform-runtime", 
    ["component", {
      "libraryName": "mint-ui",
      "style": true
    }]
  ]
}
